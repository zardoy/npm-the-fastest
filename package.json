{
    "name": "npm-the-fastest",
    "displayName": "NPM THE FASTEST",
    "description": "Full-featured extension for npm ecosystem",
    "publisher": "zardoy",
    "version": "0.0.0-dev",
    "author": "Vitaly Turovsky <vital2580@icloud.com>",
    "license": "MIT",
    "private": true,
    "preview": true,
    "icon": "resources/icon-stub.png",
    "contributes": {
        "commands": [
            {
                "command": "addPackages",
                "title": "Add npm Packages"
            },
            {
                "command": "runInstall",
                "title": "Install Workspace Dependencies"
            },
            {
                "command": "pnpmOfflineInstall",
                "category": "NPM THE FASTEST (pnpm)",
                "title": "Install Offline npm Packages"
            },
            {
                "command": "removePackages",
                "title": "Remove npm Packages"
            },
            {
                "command": "runNpmScript",
                "title": "Run npm Script"
            },
            {
                "command": "runMainNpmScript",
                "title": "Run Main npm Script"
            },
            {
                "command": "openClosestPackageJson",
                "title": "Open Closest package.json to Current Editor"
            },
            {
                "command": "openWorkspacePackageJson",
                "title": "Open package.json of current workspace"
            },
            {
                "command": "openPackageJsonFrom",
                "title": "Open package.json from..."
            },
            {
                "command": "runBinCommand",
                "title": "Run bin Command of Locally Installed Package"
            },
            {
                "command": "enforcePackageManagerUse",
                "title": "Enforce Package Manager Use"
            },
            {
                "command": "openNodeModulesFileAtGithub",
                "title": "Open node_modules file at GitHub",
                "icon": "$(github-inverted)"
            },
            {
                "command": "openPackageReadmePreview",
                "title": "Open Package Readme Preview"
            },
            {
                "command": "openPackageRepository",
                "title": "Open Package Repository"
            },
            {
                "command": "openOnNpm",
                "title": "Open on npm"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "runInstall",
                    "when": "!virtualWorkspace"
                },
                {
                    "command": "pnpmOfflineInstall",
                    "when": "!virtualWorkspace"
                },
                {
                    "command": "runMainNpmScript",
                    "when": "!virtualWorkspace"
                },
                {
                    "command": "runNpmScript",
                    "when": "!virtualWorkspace"
                },
                {
                    "command": "runBinCommand",
                    "when": "!virtualWorkspace"
                },
                {
                    "command": "openNodeModulesFileAtGithub",
                    "when": "resourcePath =~ /node_modules/"
                },
                {
                    "command": "openPackageRepository",
                    "when": "false"
                },
                {
                    "command": "openPackageReadmePreview",
                    "when": "false"
                },
                {
                    "command": "openOnNpm",
                    "when": "false"
                }
            ],
            "editor/title": [
                {
                    "command": "openNodeModulesFileAtGithub",
                    "group": "navigation",
                    "when": "resourcePath =~ /node_modules/"
                }
            ]
        },
        "configuration": {
            "properties": {
                "install.handleMutlirootWorkspaces": {
                    "type": "string",
                    "enum": [
                        "disabled",
                        "parallel",
                        "serial"
                    ],
                    "enumDescriptions": [
                        "Always perform install only for active editor workspace",
                        "Install packages for each workspace at the same time. Several progress notifications will be showed",
                        "Install packages for each workspace in sequence"
                    ],
                    "default": "parallel"
                },
                "install.watchPackageJson": {
                    "type": "boolean",
                    "description": "Invoke package maanger install whenever you change dependencies in opened package.json",
                    "default": true
                },
                "install.watchDelay": {
                    "type": "number",
                    "description": "Delay, before package manager invoke. Uses debounce. Relies on #install.watchPackageJson#",
                    "default": 500
                },
                "install.watchPackageLocks": {
                    "type": "string",
                    "description": "Invoke package manager install whenever lockfile is changed (usually after git operations)",
                    "enum": [
                        "disabled",
                        "prompt",
                        "withoutPrompt"
                    ],
                    "enumDescriptions": [
                        "Don't do anything",
                        "Display confirmation prompt to perform packages install",
                        "Perform packages install without any confirmation prompts"
                    ]
                },
                "install.runOnOpen": {
                    "type": "string",
                    "enum": [
                        "disable",
                        "always",
                        "ifNeeded",
                        "askIfNeeded"
                    ],
                    "description": "Whether to run package manager install command whenever new workspace is opened, to ensure that you have up-to-date deps",
                    "default": "disable"
                },
                "install.clipboardDetection": {
                    "type": "string",
                    "description": "What to do on clipboard detection for copied command to install packages",
                    "enum": [
                        "disabled",
                        "ask",
                        "auto"
                    ],
                    "default": "ask"
                },
                "runBinCommand.searchLocation": {
                    "type": "string",
                    "description": "From where to pick bin commands",
                    "enum": [
                        "allUpToRoot",
                        "nearest"
                    ],
                    "enumDescriptions": [
                        "All bin commands from all node_modules from cwd up to root of workspace",
                        "All bin commands from nearest node_modules"
                    ]
                },
                "yarnBerry.workspaceAutoInstall": {
                    "type": "boolean",
                    "markdownDescription": "Applies only if project uses yarn 2 or greater and #openWorkspaceAutoInstall# is withoutPrompt. In this case project script, instead of your system-wide package manager will be used",
                    "default": false
                },
                "install.packs": {
                    "type": "object",
                    "description": "Create your own sets of package to install at once",
                    "patternProperties": {
                        ".*": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            },
                            "minLength": 1
                        }
                    }
                },
                "search.provder": {
                    "type": "string",
                    "deprecationMessage": "This setting is ignored for now and will be removed",
                    "description": "Provider that will be used when adding new packages from NPM. Algolia provides more info and 2x faster",
                    "default": "algolia",
                    "enum": [
                        "algolia",
                        "npms"
                    ]
                },
                "scripts.mainScripts": {
                    "type": "array",
                    "description": "The first script will be run from this list on `start-npm-script` command.",
                    "items": {
                        "type": "string"
                    },
                    "uniqueItems": true,
                    "default": [
                        "dev",
                        "start",
                        "watch"
                    ]
                },
                "scripts.searchLocation": {
                    "type": "string",
                    "description": "This affects `runScript` and `runMainScript` commands. From where pick scripts",
                    "enum": [
                        "closest",
                        "currentWorkspaceFolderRoot"
                    ],
                    "enumDescriptions": [
                        "Closest package.json file of active text editor",
                        "Root of current workspace folder (based on active text editor in "
                    ]
                },
                "statusbar.showMainScriptStatus": {
                    "type": "boolean",
                    "default": true
                },
                "search.includeDeprecated": {
                    "type": "boolean",
                    "description": "Include deprecated packages into search results",
                    "default": false
                },
                "editor.suggestPackagesToInstall": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "boolean"
                    }
                },
                "codeAction.resolveBranchName": {
                    "type": "boolean",
                    "description": "Disable, if action open repository takes a lot of time, would use master on directory path",
                    "default": true
                }
            }
        }
    },
    "type": "commonjs",
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": false,
            "description": "This extension needs to run arbitrary commands provided by package manager, installed on your system"
        },
        "virtualWorkspaces": {
            "supported": "limited",
            "description": "In virtual workspaces (or web), only basic commands and package.json edits are supported."
        }
    },
    "activationEvents": [
        "workspaceContains:package.json",
        "onCommands"
    ],
    "categories": [
        "Other"
    ],
    "keywords": [
        "npm",
        "yarn",
        "pnpm",
        "dependencies",
        "corepack"
    ],
    "scripts": {
        "start": "vscode-framework start",
        "build": "vscode-framework build --skip-typechecking"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.0.3",
        "@types/lodash": "^4.14.178",
        "@types/mocha": "^9.0.0",
        "@types/semver": "^7.3.9",
        "@types/vscode": "^1.63.0",
        "@types/yamljs": "^0.2.31",
        "@vscode/test-electron": "^1.6.2",
        "@zardoy/tsconfig": "^1.2.2",
        "esbuild": "^0.13.15",
        "esbuild-runner": "^2.2.1",
        "eslint-plugin-zardoy-config": "^0.1.4",
        "jest": "^27.4.5",
        "mocha": "^9.1.3",
        "type-fest": "^2.8.0",
        "typescript": "^4.5.4",
        "vscode-framework": "^0.0.18"
    },
    "dependencies": {
        "@hediet/node-reload": "^0.7.3",
        "@mui/icons-material": "^5.2.1",
        "@types/hosted-git-info": "^3.0.2",
        "@types/jsonfile": "^6.0.1",
        "@types/node": "^16.11.12",
        "@zardoy/utils": "^0.0.4",
        "algoliasearch": "^4.11.0",
        "cross-env": "^7.0.3",
        "default-branch": "^1.1.1",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "execa": "^5.1.1",
        "filesize": "^8.0.6",
        "find-up": "^6.2.0",
        "fs-extra": "^10.0.0",
        "globby": "^11.0.4",
        "got": "^11.8.3",
        "hosted-git-info": "^4.0.2",
        "jsonfile": "^6.1.0",
        "lodash": "^4.17.21",
        "npm-check": "^5.9.2",
        "npms.io": "^2.1.0",
        "pkg-dir": "^5.0.0",
        "rambda": "^6.9.0",
        "semver": "^7.3.5",
        "slash": "^3.0.0",
        "ts-node": "^10.4.0",
        "vscode-uri": "^3.0.2",
        "which-pm": "^2.0.0",
        "yamljs": "^0.3.0"
    }
}
